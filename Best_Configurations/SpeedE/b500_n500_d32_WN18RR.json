{
  "dataset": "WN18RR",
  "dataset_kwargs": {
    "create_inverse_triples": true
  },
  "model": "Model",
  "model_kwargs": {
    "min_denom": 0,
    "interactionMode": "SpeedE_2_Diff_dIs0.3",
    "embedding_dim": 32,
    "p": 2,
    "tanh_map": false
  },
  "loss": "NSSALoss",
  "loss_kwargs": {
    "reduction": "sum",
    "adversarial_temperature": 2,
    "margin": 3
  },
  "optimizer": "Adam",
  "optimizer_kwargs": {
    "lr": 5e-03
  },
  "training_loop": "SLCWATrainingLoop",
  "negative_sampler": "BasicNegativeSampler",
  "negative_sampler_kwargs": {
    "num_negs_per_pos": 500
  },
  "training_kwargs": {
    "num_epochs": 1000,
    "checkpoint_frequency": 60,
    "checkpoint_name": "checkpoint.pt",
    "checkpoint_on_failure": true,
    "batch_size": 500
  },
  "stopper": "early",
  "stopper_kwargs": {
    "patience": 1000,
    "relative_delta": 0.00005,
    "frequency": 1000
  },
  "evaluator": "RankBasedEvaluator",
  "evaluator_kwargs": {
    "filtered": true,
    "batch_size": 1
  },
  "device": "gpu"
}